    <property>
      <content>{{velocity}}
#set ($sheetsMissingLicense = $services.licensing.licensor.getSheetsMissingLicense($doc.documentReference, $xcontext.action))
#if ($services.sheet.getSheets($doc, $xcontext.action).isEmpty() &amp;&amp; $sheetsMissingLicense.size() &gt; 0)
  #set ($message = '')
  #foreach ($sheet in $sheetsMissingLicense)
    #foreach ($extension in $services.extension.xar.getInstalledExtensions($sheet))
      #set ($message = $message+" "+$extension.getName())
    #end
  #end
  {{html clean="false"}}
  &lt;div class="hidden" id="licenseCheckingExtensionPoint"&gt;
  $services.localization.render("licensor.wrongDisplayedContent", [$message])
  &lt;/div&gt;
  {{/html}}
  #set($discard = $xwiki.jsx.use('Licenses.Code.UIExtensionLicenseChecking', {'minify':false}))
#end
{{/velocity}}</content>
