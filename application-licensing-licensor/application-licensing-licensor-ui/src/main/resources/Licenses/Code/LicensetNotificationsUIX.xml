    <property>
      <content>{{velocity}}
#if ($hasAdmin)
  #set ($licensorId = 'com.xwiki.licensing:application-licensing-licensor-api')
  #set ($dependencies = $services.extension.installed.getBackwardDependencies($licensorId))
  #set ($list = '')
  #foreach ($set in $dependencies.entrySet())
    #foreach ($extension in $set.value)
      #if ($extension.type == 'xar')
        #if (!$services.licensing.licensor.hasLicensureForExtension($extension.id))
          #if ($foreach.index &lt; 3)
            #set ($list = "${list}&lt;li&gt;$extension.name&lt;/li&gt;")
          #else
            #set ($list = "${list}...")
            #break
          #end
        #end
      #end
    #end
  #end
  #if ($list != '')
    #set ($discard = $xwiki.ssx.use('Licenses.Code.LicensetNotificationsUIX'))
    #set ($licensesSectionURL = $xwiki.getURL('XWiki.XWikiPreferences', 'admin', 'editor=globaladmin&amp;section=Licenses'))
    {{html clean=false}}
      &lt;li class="notifications-payingapps-item"&gt;
        &lt;p&gt;$services.localization.render('licensor.missingLicenses', ["&lt;a href='$licensesSectionURL'&gt;", '&lt;/a&gt;'])&lt;/p&gt;
        &lt;ul&gt;$list&lt;/ul&gt;
      &lt;/li&gt;
    {{/html}}
  #end
#end
{{/velocity}}</content>
